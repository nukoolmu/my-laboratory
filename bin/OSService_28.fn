OSService_28(){

    RESULT=$(rpm -q tftp-server package tftp-server is not installed |grep 'is not installed')

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Package had installed'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The package does not installed'
    fi

    result "$AUDIT_RESULT" "2.8" "Remove tftp-server"
    writeFile "$AUDIT_RESULT" "2.8" "Remove tftp-server" '=' "Package tftp-server shall be remove" "$CUR_RESULT"

}
