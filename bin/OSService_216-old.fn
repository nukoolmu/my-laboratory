OSService_216-old(){

    RESULT=$(chkconfig --list|grep echo-dgram)

    if [ ! -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "2.16" "Disable echo-dgram"
    writeFile "$AUDIT_RESULT" "2.16" "Disable echo-dgram" '=' "The Service shall be disabled" "$CUR_RESULT"

}
