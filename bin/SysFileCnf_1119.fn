SysFileCnf_1119(){
    RESULT_1=$(/sbin/modprobe -n -v hfsplus 2>/dev/null)
    RESULT_2=$(/sbin/lsmod | grep hfsplus 2>/dev/null)
    if [ -z "${RESULT_1}" ] || [ -z "${RESULT_2}" ] ;then
        AUDIT_RESULT='MANUAL'
        CUR_RESULT='The filesystem hfsplus is disabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The filesystem hfsplus is not disabled'
    fi

    result "$AUDIT_RESULT" "1.1.19" "Disable Mounting of hfsplus Filesystems"
    writeFile "$AUDIT_RESULT" "1.1.19" "Disable Mounting of hfsplus Filesystems" '=' "The mounting hfsplus filesystem shall be disabled" "$CUR_RESULT"


}