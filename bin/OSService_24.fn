OSService_24(){

    RESULT=$(rpm -q rsh |grep 'is not installed')

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Package had installed'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The package does not installed'
    fi

    result "$AUDIT_RESULT" "2.4" "Remove rsh"
    writeFile "$AUDIT_RESULT" "2.4" "Remove rsh" '=' "Package rsh shall be remove" "$CUR_RESULT"

}
