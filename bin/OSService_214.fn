OSService_214(){

    RESULT=$(systemctl is-enabled daytime-dgram |grep disabled 2>/dev/null)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "2.14" "Disable daytime-dgram"
    writeFile "$AUDIT_RESULT" "2.14" "Disable daytime-dgram" '=' "The Service shall be disabled" "$CUR_RESULT"

}
