SpService_38(){

    RESULT_1=$(systemctl is-enabled nfslock 2>/dev/null)
    RESULT_2=$(systemctl is-enabled rpcgssd 2>/dev/null)
    RESULT_3=$(systemctl is-enabled rpcbind 2>/dev/null)
    RESULT_4=$(systemctl is-enabled rpcidmapd 2>/dev/null)
    RESULT_5=$(systemctl is-enabled rpcsvcgssd 2>/dev/null)

    if [ ! -z "${RESULT_1}" ] && [ ! -z "${RESULT_2}" ] && [ ! -z "${RESULT_3}" ] && [ ! -z "${RESULT_4}" ] && [ ! -z "${RESULT_5}" ] ;then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='package and service are enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='package and service are disabled'
    fi
                                                                                                                                                                            
    result "$AUDIT_RESULT" "3.8" "Disable NFS and RPC"
    writeFile "$AUDIT_RESULT" "3.8" "Disable NFS and RPC" '=' "The NFS and RPC is not installed" "$CUR_RESULT"

}