SpService_33(){

    RESULT=$(rpm -q avahi |grep "is not installed")
    
    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='package and service is already enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='package and service is not disabled'
    fi

    result "$AUDIT_RESULT" "3.3" "Disable Avahi Server"
    writeFile "$AUDIT_RESULT" "3.3" "Service avahi-daemon shall be disabled" '=' "Service avahi-daemon is already disabled" "$CUR_RESULT"

}
