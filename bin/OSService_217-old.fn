OSService_217-old(){

    RESULT=$(chkconfig --list |grep echo-stream )

    if [ ! -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "2.17" "Disable echo-stream"
    writeFile "$AUDIT_RESULT" "2.17" "Disable echo-stream" '=' "The Service shall be disabled" "$CUR_RESULT"

}
