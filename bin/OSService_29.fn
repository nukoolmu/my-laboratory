OSService_29(){

    RESULT=$(rpm -q talk |grep 'is not installed')

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Package had installed'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The package does not installed'
    fi

    result "$AUDIT_RESULT" "2.9" "Remove talk"
    writeFile "$AUDIT_RESULT" "2.9" "Remove talk" '=' "Package talk shall be remove" "$CUR_RESULT"

}
