OSService_213(){

    RESULT=$(systemctl is-enabled chargen-stream |grep disabled) 2>/dev/null

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "2.13" " Disable chargen-stream"
    writeFile "$AUDIT_RESULT" "2.13" " Disable chargen-stream" '=' "The Service shall be disabled" "$CUR_RESULT"

}
