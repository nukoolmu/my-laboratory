SpService_35(){

    RESULT=$(rpm -q dhcp package |grep dhcp)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "3.5" "Disable DHCP Server"
    writeFile "$AUDIT_RESULT" "3.5" "Disable DHCP Server" '=' "The DHCP Server is not installed" "$CUR_RESULT"

}
