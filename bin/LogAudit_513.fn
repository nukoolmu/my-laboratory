LogAudit_513(){

    RESULT=$(ls -l /var/log)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='MANUAL'
        CUR_RESULT='The service need maunaul Configure'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The service already enabled'
    fi

    result "$AUDIT_RESULT" "5.1.3" "Configure /etc/rysylog.conf"
    writeFile "$AUDIT_RESULT" "5.1.3" "Configure /etc/rysylog.conf" '=' "Configure /etc/rysylog.conf" "$CUR_RESULT"
}
