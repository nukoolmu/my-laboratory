ConfigureSSH_623(){
    RESULT=$(/bin/ls -l /etc/ssh/sshd_config |grep rw)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT=' The password reuse is not limited'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The password reuse is limited'
    fi

    result "$AUDIT_RESULT" "6.2.3" " Limit Password Reuse"
    writeFile "$AUDIT_RESULT" "6.2.3" " Limit Password Reuse" '=' " Limit Password Reuse" "$CUR_RESULT"
}