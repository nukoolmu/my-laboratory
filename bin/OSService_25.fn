OSService_25(){

    RESULT=$(rpm -q ypbind |grep 'is not installed')

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Package had installed'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The package does not installed'
    fi

    result "$AUDIT_RESULT" "2.5" "Remove NIS Client"
    writeFile "$AUDIT_RESULT" "2.5" "Remove NIS Client" '=' "Package NIS Client shall be remove" "$CUR_RESULT"

}
