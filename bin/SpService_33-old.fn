SpService_33-old(){

    RESULT=$(chkconfig --list |grep avahi)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "3.3" "Disable Avahi Server"
    writeFile "$AUDIT_RESULT" "3.3" "Disable Avahi Server" '=' "The Avahi Server is not installed" "$CUR_RESULT"

}
