OSService_218(){

    RESULT=$(systemctl is-enabled tcpmux-server |grep disabled)

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Service was currently enabled'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The Service is disabled'
    fi

    result "$AUDIT_RESULT" "2.18" "Disable tcpmux-server"
    writeFile "$AUDIT_RESULT" "2.18" "Disable tcpmux-server" '=' "The Service shall be disabled" "$CUR_RESULT"

}
