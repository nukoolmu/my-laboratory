OSService_211(){

    RESULT=$(rpm -q xinetd |grep 'is not installed')

    if [ -z "${RESULT}" ];then
        AUDIT_RESULT='FAIL'
        CUR_RESULT='The Package had installed'
    else
        AUDIT_RESULT='PASS'
        CUR_RESULT='The package does not installed'
    fi

    result "$AUDIT_RESULT" "2.11" "Remove xinetd"
    writeFile "$AUDIT_RESULT" "2.11" "Remove xinetd" '=' "Package xinetd shall be remove" "$CUR_RESULT"

}
